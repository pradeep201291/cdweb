  "scripts": {
    "start": "webpack-dev-server --config config/webpack/webpack.nonprod.config.js --progress --profile --content-base src/client",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor config/test/protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test": "karma start config/test/karma.conf.js",
    "test-once": "tsc && karma start config/test/karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
